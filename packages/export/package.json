{
  "name": "@lua-learning/export",
  "version": "0.0.1",
  "description": "Export Lua projects as standalone HTML packages",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:canvas && tsc",
    "build:canvas": "node scripts/bundle-canvas-inline.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:mutation": "stryker run",
    "clean": "rm -rf dist",
    "lint": "eslint ."
  },
  "keywords": [
    "lua",
    "export",
    "standalone",
    "html"
  ],
  "license": "MIT",
  "dependencies": {
    "@lua-learning/lua-runtime": "*",
    "jszip": "^3.10.1",
    "luaparse": "^0.3.1",
    "wasmoon": "^1.16.0"
  },
  "peerDependencies": {
    "@lua-learning/shell-core": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@stryker-mutator/core": "^9.0.2",
    "@stryker-mutator/typescript-checker": "^9.0.2",
    "@stryker-mutator/vitest-runner": "^9.0.2",
    "esbuild": "^0.25.4",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vitest": "^4.0.15"
  }
}
